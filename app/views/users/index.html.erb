<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>First name</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.full_name, user %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Add to Group', add_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
